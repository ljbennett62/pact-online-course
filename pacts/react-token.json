{
  "consumer": {
    "name": "React"
  },
  "provider": {
    "name": "token"
  },
  "interactions": [
    {
      "description": "GET user token",
      "providerState": "user token",
      "request": {
        "method": "GET",
        "path": "/token/1235",
        "headers": {
          "Accept": "application/json, text/plain, */*"
        }
      },
      "response": {
        "status": 200,
        "headers": {
          "Content-Type": "application/json"
        },
        "body": {
          "token": "bearer"
        },
        "matchingRules": {
          "$.body.token": {
            "match": "type"
          }
        }
      }
    }
  ],
  "metadata": {
    "pactSpecification": {
      "version": "2.0.0"
    }
  }
}